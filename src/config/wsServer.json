{
    "path": "/ws",
    "maxSessions": 50,
    "timeout": 3000,
    "webSocketServer": {
        "noServer": true,
        "perMessageDeflate": {
            "zlibDeflateOptions": {
                "chunkSize": 1024,
                "memLevel": 7,
                "level": 3
            },
            "zlibInflateOptions": {
                "chunkSize": 1024
            },
            "clientNoContextTakeover": true,
            "serverNoContextTakeover": true,
            "serverMaxWindowBits": 10,
            "concurrencyLimit": 10,
            "threshold": 1024
        }
    }
}